<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ThreePageLinearLayout">
        <attr name="marginBottom" format="dimension" />
        <attr name="marginTop" format="dimension" />
    </declare-styleable>

    <attr name="picture.preview.complete.textColor" format="reference|color" />

    <declare-styleable name="OverlayListLayout">
        <attr name="item_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SuitedImageView">
        <attr name="resize_base_on" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>

    </declare-styleable>

</resources>